      {{ drupal_view('material_proximas_clases', 'block_1',id_alum ) }}
      {{ drupal_view('resumen_de_clases', 'block_2',id_alum ) }}
    </div>
    <div class="col-md-9 col-derecha">
       {{ drupal_view('logros_taxonomia', 'block_1') }}
    </div>
  </div>
 


