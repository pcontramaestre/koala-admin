{% if title is not empty %}
    <h3>{{ title }}</h3>
{% endif %}
<div id="views-bootstrap-tab-{{ id }}" {{ attributes.addClass(classes) }}>
    <ul class="nav nav-{{ tab_type }} {% if justified %}nav-justified{% endif %}">
        {% for key, tab in tabs %}
            {% set tab_classes = ['nav-item'] %}
            <li class="{{ tab_classes|join(' ') }}" role="presentation" key-tab="{{ key }}">
                <a href="#" data-bs-target="{{ "#tab-#{  id }-#{ key }" }}" type="button" role="tab" aria-controls="tab-{{ id }}-{{ key }}" aria-selected="{{ loop.first ? 'true': 'false' }}" class="nav-link {{ loop.first ? 'active' }}" data-bs-toggle="tab"><span class="name-student">{{ tab }}</span></a>
            </li> 
        {% endfor %}
    </ul>
    <div class="tab-content">
        {% for key, row in rows %}
            {% set row_classes = ['tab-pane', 'fade', loop.first ? 'show active'] %}
            {% set id_alumno = row.content['#row'].users_field_data_node__field_alumno_relacionado_uid %}
            <div class="{{ row_classes|join(' ') }}" id="tab-{{ id }}-{{ key }}" role="tabpanel" aria-labelledby="tab-{{ id }}-{{ key }}-tab" {{ row.attributes }} id-alumno="{{id_alumno}}" key-tab="{{ key }}">
                {{ row.content }}
                {# {{ dump(row.content['#row'].users_field_data_node__field_alumno_relacionado_uid )}}
                {{ id }}
                {{ key }} #}
            </div>
        {% endfor %}
    </div>
</div>
