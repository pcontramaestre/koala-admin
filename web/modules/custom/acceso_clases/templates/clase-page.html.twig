<div class="iframe-clase">
	<div
		class="cuadro-superpuesto">
		{# Twig tweak open view estrellas #}
		{% if rol == 'hijo' %}
			{{ drupal_view('estrellas', 'block_1') }}
		{% else %}
			<div class="views-element-container">
				<div class="no-admin view view-estrellas view-id-estrellas view-display-id-block_1 js-view-dom-id-14eedb4a0333f46edf063748652e1f22903c8211c4604b8f315609c3f484061d" data-once="ajax-pager">
					<div class="view-content row">
						<div class="views-row">
							<div class="views-field views-field-nothing">
